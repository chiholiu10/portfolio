{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx kill-port 3001 && next dev -p 3001",
    "build": "next build",
    "reinstall": "rm -rf node_modules yarn.lock package-lock.json && yarn install",
    "start": "npx kill-port 3000 && next start",
    "lint": "npx eslint . --ignore-pattern '.next' --ignore-pattern 'eslint.config.mjs'",
    "lint:fix": "npx eslint --fix . --ignore-pattern '.next' --ignore-pattern 'eslint.config.mjs'",
    "ci": "yarn cache clean && yarn install --frozen-lockfile",
    "test": "jest",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "commit": "cz"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "axios": "^1.4.0",
    "contentful": "^9.2.5",
    "cypress": "^13.13.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "framer-motion": "^6.3.3",
    "globals": "^15.14.0",
    "graphql": "^15.8.0",
    "next": "^14.0.4",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.18",
    "@types/react": "18.2.0",
    "eslint": "^9.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.4",
    "jest": "^29.1.0",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
